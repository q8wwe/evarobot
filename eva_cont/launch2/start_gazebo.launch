<?xml version="1.0"?>
<launch>
	<arg name="world_path" default="$(find eva_cont)/world/my.world"/>
 <include file="$(find eva_cont)/launch2/smallmaz.launch">
    <arg name="world_name" value="$(arg world_path)"/>  <!-- Note: the world_name is with respect to GAZEBO_RESOURCE_PATH environmental variable -->
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
</launch>
